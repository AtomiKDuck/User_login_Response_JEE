<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" version="4.0">
  <display-name>Exercise6</display-name>
  
   <servlet>
  	<servlet-name>peanut</servlet-name>
  	<servlet-class>login</servlet-class> <!-- Servlet name goes here (The java class) -->
  </servlet>
  
  <servlet-mapping>
  	<servlet-name>peanut</servlet-name>
  	<url-pattern>/pickle</url-pattern> <!-- Every URL pattern MUST have a slash / -->
  </servlet-mapping>
  
  
  
  <context-param>
    	<param-name>Name</param-name>
    	<param-value>atomic</param-value>
</context-param>
<context-param>
    	<param-name>Password</param-name>
    	<param-value>duck</param-value>
</context-param>
</web-app>